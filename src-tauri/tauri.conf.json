{
  "productName": "dofus-craft",
  "identifier": "fr.flapili.dofus-craft",
  "build": {
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "pnpm generate",
    "frontendDist": "../dist"
  },
  "app": {
    "macOSPrivateApi": true,
    "windows": [
      {
        "title": "Dofus-craft",
        "width": 300,
        "height": 600,
        "minHeight": 300,
        "minWidth": 200
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
